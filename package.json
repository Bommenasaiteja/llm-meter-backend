{
  "name": "llm-meter-backend",
  "version": "0.0.1",
  "description": "NestJS backend API for LLM Meter - Track and monitor LLM usage",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bommenasaiteja/llm-meter-backend.git"
  },
  "keywords": [
    "llm",
    "monitoring",
    "openai",
    "anthropic",
    "ollama",
    "token-tracking",
    "cost-analysis",
    "backend",
    "api",
    "nestjs"
  ],
  "author": "Open Source Contributors",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "typeorm migration:run -d src/database/data-source.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/platform-ws": "^10.3.0",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.6.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "*.json",
    "*.md",
    ".env.example"
  ]
}
